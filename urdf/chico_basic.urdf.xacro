<?xml version="1.0"?>
<robot name="chico_basic" xmlns:xacro="http://ros.org/wiki/xacro">

  <material name="gray">
    <color rgba="0.5 0.5 0.5 1.0"/>
  </material>

  <link name="base_link">

    <visual>
      <geometry>
        <box size="0.8 1 0.6"/>
      </geometry>
      <material name="gray"/>
    </visual>

    <collision>
      <geometry>
        <box size="0.8 1 0.6"/>
      </geometry>
    </collision> 
  <inertial>
    <origin xyz="0 0 0.3" rpy="0 0 0"/>
    <mass value="90"/> 
    <inertia
      ixx="${1.0/12.0 * 90.0 * (0.8*0.8 + 0.6*0.6)}"    
      iyy="${1.0/12.0 * 90.0 * (1.0*1.0 + 0.6*0.6)}"
      izz="${1.0/12.0 * 90.0 * (0.8*0.8 + 1.0*1.0)}"
      ixy="0.0"
      ixz="0.0"
      iyz="0.0"/>
  </inertial>
  </link>

    <!-- Braço frontal esquerdo -->
  <link name="arm_front_left">

    <visual>
      <geometry>
        <cylinder length="1.1" radius="0.1"/>
      </geometry>
      <material name="gray"/>
    </visual>

    <collision>
      <geometry>
        <cylinder length="1.1" radius="0.1"/>
      </geometry>
      </collision>

    <inertial>
      <origin xyz="0 0 0.55" rpy="0 0 0"/>
      <mass value="10"/>  

      <!-- Tensor de inércia do cilindro -->
      <inertia
        ixx="${(1.0/12.0) * 10.0 * (3*(0.1*0.1) + 1.1*1.1)}"
        iyy="${(1.0/12.0) * 10.0 * (3*(0.1*0.1) + 1.1*1.1)}"
        izz="${(1.0/2.0) * 10.0 * (0.1*0.1)}"
        ixy="0.0"
        ixz="0.0"
        iyz="0.0"/>
    </inertial>


  </link>

  <!-- Junta do braço frontal esquerdo -->
  <joint name="joint_arm_front_left" type="fixed">
    <parent link="base_link"/>
    <child link="arm_front_left"/>
    <origin xyz="-0.65 0.35 -0.3" rpy="0 0.6 0"/>
  </joint>

  <!-- Roda frontal esquerda -->
  <link name="wheel_front_left">

    <visual>
      <geometry>
        <sphere radius="0.3"/>
      </geometry>
      <material name="gray"/>
    </visual>

    <collision>
      <geometry>
        <sphere radius="0.3"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="5"/>  

      <!-- Tensor de inércia da esfera -->
      <inertia
          ixx="${(2.0/5.0) * 5.0 * (0.3*0.3)}"
          iyy="${(2.0/5.0) * 5.0 * (0.3*0.3)}"
          izz="${(2.0/5.0) * 5.0 * (0.3*0.3)}"
          ixy="0.0"
          ixz="0.0"
          iyz="0.0"/>
    </inertial>
  </link>

    <!-- Junta da roda frontal esquerda -->
  <joint name="joint_wheel_front_left" type="revolute">
    <parent link="arm_front_left"/>
    <child link="wheel_front_left"/>
    <origin xyz="0 0 -0.3" rpy="0 0 0" />
    <axis xyz="0 1 0"/>
    <limit effort="5.0" velocity="10.0"/>
  </joint>


  <!-- Braço frontal direito -->
